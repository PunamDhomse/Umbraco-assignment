@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "WebBase.cshtml";
}

<div id="fh5co-about-section">
    <div class="container">
        @{ Html.RenderAction("RenderTitleControls", "SiteLayout");}
        <div class="row">
            <div class="col-md-8">
                <div class="row">
                    <div class="col-md-12">
                        <div class="about-inner">
                            @CurrentPage.GetGridHtml("ContentGrid", "bootstrap3")
                            <blockquote>
                                <p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia,
                                there live the blind texts. Separated they live in Bookmarksgrove right at the coast of the Semantics, a large language ocean.</p>
                            </blockquote>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>